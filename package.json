{
  "name": "fcbas-front",
  "version": "1.0.0",
  "description": "Система для аналитики полетов гражданских",
  "author": "LetSquad",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "cross-env DEBUG=true DEV_SERVER=true rspack serve --mode=development",
    "dev:mock": "cross-env WITH_MOCK=true DEBUG=true DEV_SERVER=true rspack serve --mode=development",
    "prod": "npm run lint && npm run build:prod",
    "build:prod": "cross-env rspack --mode=production",
    "build:dev": "cross-env rspack --mode=development",
    "analyze": "rspack build --analyze --mode=production",
    "lint": "npm run eslint && npm run stylelint",
    "eslint": "eslint --ext .ts,.tsx src --color",
    "stylelint": "stylelint \"src/**/*.scss\"",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
    "eslint:fix": "eslint --ext .ts,.tsx src --color --fix",
    "stylelint:fix": "stylelint \"src/**/*.scss\" --fix",
    "prepare": "husky"
  },
  "engines": {
    "node": ">=20"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{css,scss}": [
      "stylelint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@floating-ui/react": "0.27.16",
    "@react-css/flex": "2.3.0",
    "@react-keycloak/web": "3.4.0",
    "@reduxjs/toolkit": "2.9.0",
    "ajv": "8.17.1",
    "axios": "1.12.2",
    "axios-mock-adapter": "2.1.0",
    "classnames": "2.5.1",
    "file-saver": "2.0.5",
    "formik": "2.4.6",
    "keycloak-js": "26.2.0",
    "lodash.merge": "4.6.2",
    "luxon": "3.7.2",
    "primereact": "10.9.7",
    "react-is": "19.1.1",
    "react": "19.1.1",
    "react-calendar": "6.0.0",
    "react-datepicker": "8.7.0",
    "react-dom": "19.1.1",
    "react-dropzone": "14.3.8",
    "react-redux": "9.2.0",
    "react-responsive": "10.0.1",
    "react-router": "7.9.1",
    "recharts": "3.2.1",
    "recharts-to-png": "3.0.1",
    "redux-promise-middleware": "6.2.0",
    "semantic-ui-react": "3.0.0-beta.2",
    "ts-enum-util": "4.1.0",
    "yup": "1.7.0"
  },
  "devDependencies": {
    "@eslint/js": "9.36.0",
    "@rspack/cli": "1.5.5",
    "@rspack/core": "1.5.5",
    "@rspack/plugin-react-refresh": "1.5.1",
    "@stylistic/eslint-plugin": "3.1.0",
    "@types/file-saver": "2.0.7",
    "@types/lodash.merge": "4.6.9",
    "@types/luxon": "3.7.1",
    "@types/react": "19.1.13",
    "@types/react-dom": "19.1.9",
    "@types/react-redux": "7.1.34",
    "@typescript-eslint/eslint-plugin": "8.44.0",
    "@typescript-eslint/parser": "8.44.0",
    "clean-webpack-plugin": "4.0.0",
    "cross-env": "10.0.0",
    "css-loader": "7.1.2",
    "dotenv": "17.2.2",
    "eslint": "9.36.0",
    "eslint-config-airbnb-extended": "2.3.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import-x": "4.16.1",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-unicorn": "61.0.2",
    "glob": "11.0.3",
    "husky": "9.1.7",
    "prettier": "3.6.2",
    "react-refresh": "0.17.0",
    "sass-embedded": "1.92.1",
    "sass-loader": "16.0.5",
    "stylelint": "16.24.0",
    "stylelint-config-sass-guidelines": "12.1.0",
    "stylelint-order": "7.0.0",
    "stylelint-scss": "6.12.1",
    "ts-checker-rspack-plugin": "1.1.5",
    "ts-node": "10.9.2",
    "typescript": "5.9.2",
    "typescript-eslint": "8.44.0"
  }
}
